<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <strong>Username</strong>
  <br>
  <% @users.each do |user| %>
    <%= user.username %>
    <%= link_to "Show", user_path(user) %>
    <%= link_to "Edit", edit_user_path(user) %>
    <%= link_to "Delete", user, method: :delete %>

    <br>
  <% end %>
</div>
<br>
<%= link_to "New user", new_user_path %>
