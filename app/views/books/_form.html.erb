<table>
<tr>
    <th>Title</th>
    <td><%= f.text_field(:title) %></td>
</tr>
<tr>
    <th>Author</th>
    <td><%= f.text_field(:author) %></td>
</tr>
<tr>
    <th>Price</th>
    <td><%= f.text_field(:price) %></td>
</tr>
<tr>
    <th>Publish-Date</th>
    <td><%= f.date_select(:publish, start_year: 1900, end_year: Time.now.year) %></td>
</tr>     
</table>